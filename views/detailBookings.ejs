<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <title>Modernize Free</title>
  <link rel="shortcut icon" type="image/png" href="../assets/images/logos/favicon.png" />
  <link rel="stylesheet" href="../css/styles.min.css" />
  <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.10.5/font/bootstrap-icons.css">
  

</head>

<body>
  <!--  Body Wrapper -->
  <div class="page-wrapper" id="main-wrapper" data-layout="vertical" data-navbarbg="skin6" data-sidebartype="full"
    data-sidebar-position="fixed" data-header-position="fixed">
    <!-- Sidebar Start -->
    <aside class="left-sidebar">
      <!-- Sidebar scroll-->

      <%- include("../partials/sidebar.ejs") %>

        <!-- End Sidebar scroll-->
    </aside>
    <!--  Sidebar End -->
    <!--  Main wrapper -->
    <div class="body-wrapper">
      <!--  Header Start -->

      <%- include("../partials/header.ejs") %>

        <!--  Header End -->
        <div class="container-fluid">
          <div class="card">
            <div class="card-body">

              <table class="table">
                <div>
                  <h4>Detail of Booking  (<%=result.customer_id%>)</h4>
                  <div class="mt-4">
                    <h5><b>Transfer</b> :</h5>
                    <p class="mt-3">
                      <%=result.transfer_id%>
                    </p>
                  </div>

                  <div class="mt-4">
                    <h5><b>Transfer Car</b> :</h5>
                    <p class="mt-3">
                      <%-result.transfer_car%>
                    </p>
                  </div>

                  <div class="mt-4">
                    <h5><b>Country</b> :</h5>
                    <p class="mt-3">
                      <%-result.country%>
                    </p>
                  </div>

                  <div class="row mt-5">

                 

                  <div class="col-md-6">
      
                    <h4>Going Details</h4>
                 

                  <div class="mt-5">
                    <h5><b>Guest Name</b> :</h5>
                    <p class="mt-3">
                      <%-result.guest_name1%>
                    </p>
                  </div>

                  <div class="mt-4">
                    <h5><b>Guest Number</b> :</h5>
                    <p class="mt-3">
                      <%-result.guest_number1%>
                    </p>
                  </div>

                  <div class="mt-4">
                    <h5><b>Adult Age</b> :</h5>
                    <p class="mt-3">
                      <%-result.adult_age1%>
                    </p>
                  </div>

                  <div class="mt-4">
                    <h5><b>Children Age</b> :</h5>
                    <p class="mt-3">
                      <%-result.children_age1%>
                    </p>
                  </div>

                  <div class="mt-4">
                    <h5><b>Flight Number</b> :</h5>
                    <p class="mt-3">
                      <%-result.flight_number1%>
                    </p>
                  </div>
                  <div class="mt-4">
                    <h5><b>Flight Time</b> :</h5>
                    <p class="mt-3">
                      <%-result.flight_time1%>
                    </p>
                  </div>
                  <div class="mt-4">
                    <h5><b>Flight Arrival Airport</b> :</h5>
                    <p class="mt-3">
                      <%-result.flight_airport1%>
                    </p>
                  </div>

                  <div class="mt-4">
                    <h5><b>Drop Location</b> :</h5>
                    <p class="mt-3">
                      <%-result.drop1%>
                    </p>
                  </div>

                  <div class="mt-4">
                    <h5><b>Pickup Location</b> :</h5>
                    <p class="mt-3">
                      <%-result.pickup1%>
                    </p>
                  </div>

                </div>
                <div class="col-md-6">

                  <h3>Return Details</h3>

                  <div class="mt-5">
                    <h5><b>Guest Name</b> :</h5>
                    <p class="mt-3">
                      <%-result.guest_name2%>
                    </p>
                  </div>

                  <div class="mt-4">
                    <h5><b>Guest Number</b> :</h5>
                    <p class="mt-3">
                      <%-result.guest_number2%>
                    </p>
                  </div>

                  <div class="mt-4">
                    <h5><b>Adult Age</b> :</h5>
                    <p class="mt-3">
                      <%-result.adult_age2%>
                    </p>
                  </div>

                  <div class="mt-4">
                    <h5><b>Children Age</b> :</h5>
                    <p class="mt-3">
                      <%-result.children_age2%>
                    </p>
                  </div>

                  <div class="mt-4">
                    <h5><b>Flight Number</b> :</h5>
                    <p class="mt-3">
                      <%-result.flight_number2%>
                    </p>
                  </div>
                  <div class="mt-4">
                    <h5><b>Flight Time</b> :</h5>
                    <p class="mt-3">
                      <%-result.flight_time2%>
                    </p>
                  </div>
                  <div class="mt-4">
                    <h5><b>Flight Arrival Airport</b> :</h5>
                    <p class="mt-3">
                      <%-result.flight_airport2%>
                    </p>
                  </div>

                  <div class="mt-4">
                    <h5><b>Drop Location</b> :</h5>
                    <p class="mt-3">
                      <%-result.drop2%>
                    </p>
                  </div>

                  <div class="mt-4">
                    <h5><b>Pickup Location</b> :</h5>
                    <p class="mt-3">
                      <%-result.pickup2%>
                    </p>
                  </div>
                </div>
              </div>

                </div>

                <a href="/bookings" class="btn btn-primary" style="margin-left: auto;">Back</a>

              </table>
            </div>
          </div>
        </div>
    </div>
  </div>
  <script src="../js/jquery.min.js"></script>
  <script src="../js/bootstrap.bundle.min.js"></script>
  <script src="../js/sidebarmenu.js"></script>
  <script src="../js/app.min.js"></script>
  <script src="../js/simplebar.js"></script>
</body>


