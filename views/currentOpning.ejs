<!-- Added by HTTrack -->
<meta http-equiv="content-type" content="text/html;charset=utf-8" /><!-- /Added by HTTrack -->

<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <title>Modernize Free</title>
  <link rel="shortcut icon" type="image/png" href="../assets/images/logos/favicon.png" />
  <link rel="stylesheet" href="../css/styles.min.css" />
  <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.10.5/font/bootstrap-icons.css">

</head>

<body>
  <!--  Body Wrapper -->
  <div class="page-wrapper" id="main-wrapper" data-layout="vertical" data-navbarbg="skin6" data-sidebartype="full"
    data-sidebar-position="fixed" data-header-position="fixed">
    <!-- Sidebar Start -->
    <aside class="left-sidebar">
      <!-- Sidebar scroll-->

      <%- include("../partials/sidebar.ejs") %>

        <!-- End Sidebar scroll-->
    </aside>
    <!--  Sidebar End -->
    <!--  Main wrapper -->
    <div class="body-wrapper">
      <!--  Header Start -->

      <%- include("../partials/header.ejs") %>

        <!--  Header End -->
        <div class="container-fluid">
          <div class="card">
            <div class="card-body">
            
              <table class="table">
                <div style="display: flex">
                  <h5>Current Opning</h5>
                  <a href="/addCurrentOpning" class="btn btn-primary" style="margin-left: auto;">Add</a>
                </div>
                <thead>
                  <tr>
                 
                    <th scope="col">Title</th>
                    <th scope="col">Background Image</th>
                    <th scope="col">Exp</th>
                    <th scope="col">Location</th>
                    <th scope="col">Status</th>
                    <th scope="col">Action</th>
                  </tr>
                </thead>
                <tbody>
                 
                  <% result.forEach((result,index)=>{ %>
                    <tr>
  
                      <th scope="col"><%=result.title%></th>
                      <th scope="col"><img style="width: 100px;" src="../img/<%=result.image%>" alt=""></th>
                      <th scope="col"><%=result.exp%></th>
                      <th scope="col"><%=result.location%></th>
                      <th scope="col"><a class="btn btn-primary" href="/changeStatusCurrentOpning"><%=result.status%></a></th>
                      <th scope="row">
                        <a href="/detailCurrentOpning/<%=result.generateId%>" class="btn btn-primary"><i class="bi bi-eye-fill"></i></a>
                        <a href="/editCurrentOpning/<%=result.generateId%>" class="btn btn-primary"><i
                            class="bi bi-pencil-square"></i></a>
                       
                        <a href="/deleteCurrentOpning/<%=result.generateId%>" class="btn btn-danger"><i class="bi bi-trash3"></i></a>
                      </th>
                    </tr>
                    <% }) %>
                  

                </tbody>
              </table>
            </div>
          </div>
        </div>
    </div>
  </div>
  <script src="../js/jquery.min.js"></script>
  <script src="../js/bootstrap.bundle.min.js"></script>
  <script src="../js/sidebarmenu.js"></script>
  <script src="../js/app.min.js"></script>
  <script src="../js/simplebar.js"></script>
</body>


