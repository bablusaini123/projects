<meta http-equiv="content-type" content="text/html;charset=utf-8" /><!-- /Added by HTTrack -->

<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <title>Modernize Free</title>
  <link rel="shortcut icon" type="image/png" href="../assets/images/logos/favicon.png" />
  <link rel="stylesheet" href="../css/styles.min.css" />
  <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.10.5/font/bootstrap-icons.css">
  

</head>

<body>
  <!--  Body Wrapper -->
  <div class="page-wrapper" id="main-wrapper" data-layout="vertical" data-navbarbg="skin6" data-sidebartype="full"
    data-sidebar-position="fixed" data-header-position="fixed">
    <!-- Sidebar Start -->
    <aside class="left-sidebar">
      <!-- Sidebar scroll-->

      <%- include("../partials/sidebar.ejs") %>

        <!-- End Sidebar scroll-->
    </aside>
    <!--  Sidebar End -->
    <!--  Main wrapper -->
    <div class="body-wrapper">
      <!--  Header Start -->

      <%- include("../partials/header.ejs") %>

        <!--  Header End -->
        <div class="container-fluid">
          <div class="card">
            <div class="card-body">

              <table class="table">
                <div>
                  <h4>Detail of Attractions  (<%=result.title%>)</h4>
                  <div class="mt-4">
                    <h5><b>Attractions Category</b> :</h5>
                    <p class="mt-3">
                      <%=result.attraction_category%>
                    </p>
                  </div>

                  <div class="mt-4">
                    <h5><b>Title</b> :</h5>
                    <p class="mt-3">
                      <%-result.title%>
                    </p>
                  </div>

                  <div class="mt-4">
                    <h5><b>Description</b> :</h5>
                    <p class="mt-3">
                      <%-result.description%>
                    </p>
                  </div>

                  <div class="mt-4">
                    <h5><b>Adult Ticket Price</b> :</h5>
                    <p class="mt-3">
                      <%-result.adultticketprice%>
                    </p>
                  </div>

                  <div class="mt-4">
                    <h5><b>Children Ticket Price</b> :</h5>
                    <p class="mt-3">
                      <%-result.childrenticketprice%>
                    </p>
                  </div>

                  <div class="mt-4">
                    <h5><b>Opening time</b> :</h5>
                    <p class="mt-3">
                      <%-result.openingtime%>
                    </p>
                  </div>

                  <div class="mt-4">
                    <h5><b>Closing time</b> :</h5>
                    <p class="mt-3">
                      <%-result.closingtime%>
                    </p>
                  </div>

                  <div class="mt-4">
                    <h5><b>Status</b> :</h5>
                    <p class="mt-3">
                      <%-result.status%>
                    </p>
                  </div>


                </div>

                <a href="/attractions" class="btn btn-primary" style="margin-left: auto;">Back</a>

              </table>
            </div>
          </div>
        </div>
    </div>
  </div>
  <script src="../js/jquery.min.js"></script>
  <script src="../js/bootstrap.bundle.min.js"></script>
  <script src="../js/sidebarmenu.js"></script>
  <script src="../js/app.min.js"></script>
  <script src="../js/simplebar.js"></script>
</body>


