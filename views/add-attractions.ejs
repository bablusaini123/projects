  <head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <title>Travling</title>
    <link
      rel="shortcut icon"
      type="image/png"
      href="../assets/images/logos/favicon.png"
    />
    <link rel="stylesheet" href="../css/styles.min.css" />
  </head>

  <body>
    <!--  Body Wrapper -->
    <div
      class="page-wrapper"
      id="main-wrapper"
      data-layout="vertical"
      data-navbarbg="skin6"
      data-sidebartype="full"
      data-sidebar-position="fixed"
      data-header-position="fixed"
    >
      <!-- Sidebar Start -->
      <aside class="left-sidebar">
        <!-- Sidebar scroll-->

        <%- include("../partials/sidebar.ejs") %>

        <!-- End Sidebar scroll-->
      </aside>
      <!--  Sidebar End -->
      <!--  Main wrapper -->
      <div class="body-wrapper">
        <!--  Header Start -->
        <%- include("../partials/header.ejs") %>

        <!--  Header End -->
        <div class="container-fluid">
          <div class="container-fluid">
            <div class="card">
              <div class="card-body">
                <h5 class="card-title fw-semibold mb-4">Add Attractions</h5>
                <div class="card">
                  <div class="card-body">
                    <form action="/addAttractions" method="post">

                      <div class="row mb-3">
                        <label
                          class="col-sm-2 col-form-label"
                          for="basic-default-name"
                          >Select Attractions Category</label
                        >
                        <div class="col-sm-10">
                          <select name="category" id="carSelect" class="form-control" required>
                            <option value="">--please select attraction category--</option>
                            <%result.forEach((result)=>{   %>
                              <option value="<%=result.generateId%>" class="form-control">
                                <%=result.title%>
                              </option>
                         <%   })%>
                         
                          </select>
                        </div>
                      </div>
                   
                      <div class="row mb-3">
                        <label
                          class="col-sm-2 col-form-label"
                          for="basic-default-name"
                          >Title</label
                        >
                        <div class="col-sm-10">
                          <input
                            type="text"
                            class="form-control"
                            id="basic-default-name"
                            name="title"
                            required
                          />
                        </div>
                      </div>

                    

                      <div class="row mb-3">
                        <label
                          class="col-sm-2 col-form-label"
                          for="basic-default-name"
                          >Description</label
                        >
                        <div class="col-sm-10">
                          <input
                            type="text"
                            class="form-control"
                            id="basic-default-name"
                            name="description"
                            required
                          />
                        </div>
                      </div>

                      <div class="row mb-3">
                        <label
                          class="col-sm-2 col-form-label"
                          for="basic-default-name"
                          >Adult Ticket Price</label
                        >
                        <div class="col-sm-10">
                          <input
                            type="number"
                            class="form-control"
                            id="basic-default-name"
                            name="adultTicketPrice"
                            required
                          />
                        </div>
                      </div>


                      <div class="row mb-3">
                        <label
                          class="col-sm-2 col-form-label"
                          for="basic-default-name"
                          >Children Ticket Price</label
                        >
                        <div class="col-sm-10">
                          <input
                            type="number"
                            class="form-control"
                            id="basic-default-name"
                            name="childrenTicketPrice"
                            required
                          />
                        </div>
                      </div>

                      <div class="row mb-3">
                        <label
                          class="col-sm-2 col-form-label"
                          for="basic-default-name"
                          >Opening Time</label
                        >
                        <div class="col-sm-10">
                          <input
                            type="time"
                            class="form-control"
                            id="basic-default-name"
                            name="openTime"
                            required
                          />
                        </div>
                      </div>

                      <div class="row mb-3">
                        <label
                          class="col-sm-2 col-form-label"
                          for="basic-default-name"
                          >Closing Time</label
                        >
                        <div class="col-sm-10">
                          <input
                            type="time"
                            class="form-control"
                            id="basic-default-name"
                            name="closeingTime"
                            required
                          />
                        </div>
                      </div>

                      <div class="row mb-3">
                        <label for="exampleInputEmail1" class="form-label col-md-2">Select Status</label>
                        <div class="col-md-10">
                          <select name="status" id="" class="form-control" required>
                            <option value="">--select status--</option>
                           
                              <option value="Active" class="form-control">
                                  Active
                              </option>
                              <option value="Deactive" class="form-control">
                                Deactive
                            </option>
                        
                         
                          </select>
                        </div>
                      </div>

                      <div class="row justify-content-end">
                        <div class="col-sm-10">
                          <button type="submit" class="btn btn-primary">
                            Add
                          </button>
                        </div>
                      </div>
                    </form>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
    <script src="../js/jquery.min.js"></script>
    <script src="../js/bootstrap.bundle.min.js"></script>
    <script src="../js/sidebarmenu.js"></script>
    <script src="../js/app.min.js"></script>
    <script src="../js/simplebar.js"></script>
  </body>

