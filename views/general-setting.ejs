        <script type="text/javascript">

            $(function () {

                $(".upImage").change(function () {

                    var ext = $(this).val().split('.').pop().toLowerCase();
                    if ($.inArray(ext, ['gif', 'png', 'jpg', 'jpeg']) == -1) {
                        alert('Please select a valid image [ jpg | jpeg | gif | png ]');
                        $(this).val('');
                        clearImage();

                    } else {
                        $('#myImg1').hide();
                        if (this.files && this.files[0]) {
                            var reader = new FileReader();
                            reader.onload = imageIsLoaded;
                            reader.readAsDataURL(this.files[0]);
                            $(".cropFrame").css("display", "block");
                        }
                    }
                });
            });
            function imageIsLoaded(e) {
                $('#myImg').attr('src', e.target.result);
                $('#myImg').show();
                $('.cropimage').cropbox({ width:'' , height:'' , showControls: 'auto' }).on('cropbox', function (event, results, img) {
                    $('#fileinp').val(img.getDataURL());
                });
            }

            function clearImage() {
                $("#banner").val('');
                $("#myImg").removeAttr('src');
                $('#myImg').hide();
                $(".cropFrame").css("display", "none");
            }



        </script>
